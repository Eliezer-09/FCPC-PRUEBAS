na\"\r\n    },\r\n  });\r\n\r\n  dialogRef.afterClosed().subscribe((result) => {\r\n    this.changeController.emit({action:'carga',data:result})\r\n  });\r\n}\r\n\r\n}\r\n\r\n"," <ngx-spinner\r\n    bdColor=\"rgba(51,51,51,0.8)\"\r\n    size=\"medium\"\r\n    color=\"#fff\"\r\n    type=\"ball-scale-multiple\">\r\n    <p style=\"font-size: 20px; color: white\">Cargando...</p>\r\n</ngx-spinner>\r\n \r\n\r\n<div class=\"card\" style=\"padding: 10px; margin: auto;text-align: center;\">\r\n    <p><b>CARGAR EXCEL DE LA NÓMINA</b></p>\r\n    <div >\r\n        <vex-files-upload [extensions]=\"['xls','xlsx']\"></vex-files-upload>\r\n    </div>\r\n    <br>\r\n        <button\r\n            mat-raised-button=\"mat-raised-button\"\r\n            color=\"primary\"\r\n            type=\"button\"\r\n            id=\"excel\"\r\n            class=\"forms-full-button button-download\"\r\n            (click)=\"generar()\">\r\n            <mat-icon style=\"height: 18px\" [icIcon]=\"icroundFileUpload\"></mat-icon>\r\n            <span style=\"margin-left: 4px\">Cargar Ingresos/Egresos</span>\r\n        </button>\r\n</div> \r\n\r\n\r\n    <vex-ingresos-egresos *ngIf=\"ingresos_egresos.length !== 0\"\r\n        [habilitarGuardar]=\"habilitarGuardar\"\r\n        [dataFondoSource]=\"ingresos_egresos\"\r\n        [isLoading]=\"isLoading\"\r\n        [title]=\"'Ingresos/Egresos Mensuales'\"\r\n        [footerdata]=\"totales\"\r\n        (guardar)=\"solicitarCargaAportes($event)\"></vex-ingresos-egresos> \r\n        \r\n<div *ngIf=\"showNoDataAportes\">\r\n    <vex-info-message\r\n        [message]='infoMessage'\r\n        [icon]='icroundRequestPage'\r\n        [typeicon]=\"'icIcon'\"></vex-info-message>\r\n</div>\r\n","import { Component, Inject, OnInit } from \"@angular/core\";\r\nimport {\r\n  FormBuilder,\r\n  FormControl,\r\n  FormGroup,\r\n  Validators,\r\n} from \"@angular/forms\";\r\nimport { MatDialogRef, MAT_DIALOG_DATA } from \"@angular/material/dialog\";\r\nimport { Parentesco } fr