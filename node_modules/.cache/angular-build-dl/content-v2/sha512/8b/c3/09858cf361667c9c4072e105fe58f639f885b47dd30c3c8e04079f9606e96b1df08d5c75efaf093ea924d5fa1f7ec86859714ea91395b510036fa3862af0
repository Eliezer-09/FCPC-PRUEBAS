\r\n  constructor(\r\n    private fb: FormBuilder,\r\n    public dialogRef: MatDialogRef<AgregarDiaNoLaboralComponent>,\r\n    @Inject(MAT_DIALOG_DATA) public data: any,\r\n    private utilService: UtilsService\r\n  ) { }\r\n\r\n  ngOnInit(): void {\r\n    this.data.data.action = \"Agregar\";\r\n    if (this.data && this.data.data) {\r\n      const { start, end, nombre } = this.data.data;\r\n      const fechaInicioDate = new Date(start);\r\n      const fechaFinalDate =  new Date(end)\r\n      this.form.patchValue({\r\n        nombre: nombre,\r\n        start: fechaInicioDate,\r\n        end: fechaFinalDate\r\n      });\r\n    }\r\n  }\r\n\r\n  onNoClick(): void {\r\n    this.dialogRef.close();\r\n  }\r\n\r\n  enviarFormulario(): void {\r\n    if (this.form.valid) {\r\n      this.dialogRef.close(this.form.value);\r\n      this.utilService.alerta('success', 'Dia no laborable enviado correctamente.');\r\n    } else {\r\n      console.log('Formulario no válido');\r\n    }\r\n  }\r\n\r\n}\r\n","<div mat-dialog-content>\r\n    <div class=\"modal-header\">\r\n        <h4 class=\"modal-title\" id=\"modal-title\" *ngIf = \"data.action == 'Editar'\">Editar día no laborable</h4>\r\n        <h4 class=\"modal-title\" id=\"modal-title\" *ngIf = \"data.action !== 'Editar'\">Agregar día no laborable</h4>\r\n    </div>  \r\n    <form [formGroup]=\"form\">\r\n        <mat-divider></mat-divider>\r\n        <mat-form-field class=\"example-full-width\">\r\n            <mat-label>Nombre</mat-label>\r\n            <input [disabled]=\"data.visualizationMode\" matInput formControlName=\"nombre\">\r\n            <mat-error *ngIf=\"form.get('nombre').hasError('required')\">\r\n                El campo es requerido\r\n            </mat-error>\r\n        </mat-form-field>\r\n        <mat-form-field class=\"example-full-width\">\r\n            <mat-label>Rango de Fecha</mat-label>\r\n            <mat-date-range-input [formGroup]=\"form\" [rangePicker]=\"picker\">\r\n                <input matSta