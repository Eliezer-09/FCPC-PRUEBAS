e(event)\r\n          this.parameters.controls.mes.setValue(event.month() + 1);\r\n          this.parameters.controls.anio.setValue(event.year());\r\n          this.parameters.controls.codigoNomina.setValue(null);\r\n      } else {\r\n        this.parameters.controls.mes.setValue(null);\r\n        this.parameters.controls.anio.setValue(null);\r\n      }\r\n  }\r\n\r\n  \r\n  reset(){\r\n    this.parameters.controls.codigoNomina.setValue(null);\r\n    this.date.setValue(moment());\r\n    this.parameters.controls.mes.setValue(this.date.value.month() + 1);\r\n    this.parameters.controls.anio.setValue(this.date.value.year());\r\n  }\r\n\r\n\r\n}\r\n\r\n"," <ngx-spinner\r\n    bdColor=\"rgba(51,51,51,0.8)\"\r\n    size=\"medium\"\r\n    color=\"#fff\"\r\n    type=\"ball-scale-multiple\">\r\n    <p style=\"font-size: 20px; color: white\">Cargando...</p>\r\n</ngx-spinner>\r\n \r\n\r\n<div class=\"card overflow-auto -mt-16\" style=\"padding: 10px; margin: auto; text-align: center;\">\r\n    <p><b>HISTORIAL NMINA</b></p>\r\n    <form [formGroup]=\"parameters\">\r\n\r\n        \r\n  <div class=\"mat-selects\" >\r\n\r\n    <mat-form-field class=\"flex-100\" fxFlex=\"auto\">\r\n      <mat-label>Tipo Colaborador</mat-label>\r\n      <mat-select formControlName=\"idTipoColaborador\">\r\n        <mat-option [value]=\"1\">REDEP</mat-option>\r\n        <mat-option [value]=\"2\">Honorarios</mat-option>\r\n      </mat-select>\r\n    </mat-form-field>\r\n\r\n\r\n    <vex-mat-datepicker-month-year \r\n    (date_month_year_value)=\"setMonthAndYear($event)\"></vex-mat-datepicker-month-year>\r\n\r\n\r\n</div>\r\n</form>\r\n<br>\r\n      <button\r\n          mat-raised-button=\"mat-raised-button\"\r\n          [disabled]=\"(!parameters.value.mes || !parameters.value.anio)  || parameters.invalid\"\r\n          color=\"primary\"\r\n          type=\"button\"\r\n          id=\"excel\"\r\n          class=\"forms-full-button button-download\"\r\n          (click)=\"descargarNomina()\">\r\n      