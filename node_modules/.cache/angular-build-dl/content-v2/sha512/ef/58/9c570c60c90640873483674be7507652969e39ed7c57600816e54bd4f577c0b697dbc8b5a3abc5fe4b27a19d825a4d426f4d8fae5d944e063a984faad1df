ent): void {\r\n      if (event) {\r\n          this.date.setValue(event)\r\n          this.parameters.controls.mes.setValue(event.month() + 1);\r\n          this.parameters.controls.anio.setValue(event.year());\r\n          this.parameters.controls.codigoNomina.setValue(null);\r\n      } else {\r\n        this.parameters.controls.mes.setValue(null);\r\n        this.parameters.controls.anio.setValue(null);\r\n      }\r\n  }\r\n\r\n  \r\n  reset(){\r\n    this.parameters.controls.codigoNomina.setValue(null);\r\n    this.date.setValue(moment());\r\n    this.parameters.controls.mes.setValue(this.date.value.month() + 1);\r\n    this.parameters.controls.anio.setValue(this.date.value.year());\r\n  }\r\n\r\n\r\n}\r\n\r\n"," <ngx-spinner\r\n    bdColor=\"rgba(51,51,51,0.8)\"\r\n    size=\"medium\"\r\n    color=\"#fff\"\r\n    type=\"ball-scale-multiple\">\r\n    <p style=\"font-size: 20px; color: white\">Cargando Ingresos/Egresos...</p>\r\n</ngx-spinner>\r\n \r\n\r\n<div class=\"card overflow-auto -mt-16\" >\r\n    <div class=\"head-options\">\r\n        <div class=\"search-bar\">\r\n\r\n      <button\r\n            (click)=\"changeController.emit({action:'buscar',data:null})\"\r\n            class=\"head-buttons\"\r\n            mat-button=\"mat-button\">\r\n            <mat-icon  [icIcon]=\"icroundFileUpload\"></mat-icon>\r\n        </button>\r\n        </div>\r\n    </div>\r\n    <br>\r\n    <p><b>BUSCAR NMINA NO PROCESADA</b></p>\r\n    <form [formGroup]=\"parameters\">\r\n\r\n        \r\n  <div class=\"mat-selects\" >\r\n    <mat-form-field class=\"flex-100\" fxFlex=\"auto\">\r\n        <mat-label>Filtro:</mat-label>\r\n        <mat-select formControlName=\"filtro\">\r\n          <mat-option [value]=\"true\">Fecha</mat-option>\r\n          <mat-option [value]=\"false\">Cdigo</mat-option>\r\n        </mat-select>\r\n      </mat-form-field>\r\n\r\n\r\n    <vex-mat-datepicker-month-year *ngIf=\"parameters.value.filtro\"\r\n    (date_month_year_value)=\"setMonthAndYear($ev