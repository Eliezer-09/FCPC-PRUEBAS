\n    if (event.action === 'Editar') {\r\n      console.log(event)\r\n      this.openDialog( event);\r\n   }   \r\n    if (event.action === 'delete') {\r\n      this.eliminarDescuento(event.data);\r\n    }\r\n  }\r\n  \r\n  onPaginateChange(event: PageEvent) {\r\n    this.pageSize = event.pageSize;\r\n    let page = event.pageIndex;\r\n    let size = event.pageSize;\r\n    page = page + 1;\r\n    this.cargarDescuento(page, size);\r\n  }\r\n\r\n  eliminarDescuento(element: any) {\r\n    this.utilsService\r\n      .confirmar(\r\n        \"Eliminar descuento\",\r\n        \"Est seguro de eliminar este descuento?\"\r\n      )\r\n      .then((result) => {\r\n        if (result.isConfirmed) {\r\n          this.utilsService.alerta('success', 'Descuento eliminado correctamente.');\r\n        }\r\n      });\r\n  }\r\n  \r\n  cargarDescuento(page, size, term = '') {\r\n    this.isLoading = true;\r\n\r\n\r\n    this.isLoading = false;\r\n  }\r\n  \r\n\r\n  openDialog(item: any): void {\r\n    const dialogRef = this.dialog.open(AgregarDescuentoComponent, {\r\n      data:  item \r\n    });\r\n  }\r\n  \r\n  \r\n}\r\n","\r\n<ngx-spinner\r\n    bdColor=\"rgba(51,51,51,0.8)\"\r\n    size=\"medium\"\r\n    color=\"#fff\"\r\n    type=\"ball-scale-multiple\"\r\n>\r\n    <p style=\"font-size: 20px; color: white\">Generando...</p>\r\n</ngx-spinner>\r\n\r\n\r\n\r\n    <div class=\"card\">\r\n        <div class=\"head-app-bar\">\r\n            <h2 class=\"head-title\">\r\n                Descuentos\r\n            </h2>        \r\n            <div class=\"head-options\">\r\n                <div class=\"search-bar\">\r\n                    <input     \r\n                        [(ngModel)]=\"filterValue\"\r\n                        class=\"px-4 py-3 border-0 outline-none w-full bg-transparent\"\r\n                        placeholder=\"Buscar...\"\r\n                        type=\"search\"\r\n                        (keyup.enter)=\"searchTerm(filterValue)\">\r\n                        <butto