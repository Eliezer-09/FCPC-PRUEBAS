        });\r\n    } else {\r\n      this.dialogRef.close(data);\r\n    }\r\n  }\r\n}\r\n","<div mat-dialog-content>\r\n\r\n  <div class=\"modal-header\">\r\n\r\n    <h4 *ngIf=\"!data.visualizationMode\" class=\"modal-title\" id=\"modal-title\"> {{tipoModificacion}}</h4>\r\n    <h4 *ngIf=\"data.visualizationMode\">Ver área</h4>\r\n  </div>\r\n\r\n\r\n  <form [formGroup]=\"formAddAreaSubArea\">\r\n\r\n\r\n    <mat-form-field class=\"example-full-width\">\r\n      <mat-label>Nombre de área</mat-label>\r\n      <input [readonly]=\"data.visualizationMode\" (keydown)=\"utilsService.validacionSoloLetras($event)\" matInput\r\n        formControlName=\"descripcion\" required>\r\n    </mat-form-field>\r\n\r\n\r\n\r\n    <mat-form-field class=\"example-full-width\">\r\n      <mat-label>Cargo Supervisor</mat-label>\r\n      <mat-select required formControlName=\"idCargo\">\r\n        <mat-option [disabled]=\"data.visualizationMode\">\r\n          <ngx-mat-select-search placeholderLabel=\"Buscar cargos...\"\r\n            noEntriesFoundLabel=\"No se encuentra el tipo de cargo\" [formControl]=\"bankFilterCtrl\">\r\n          </ngx-mat-select-search>\r\n        </mat-option>\r\n        <mat-option [disabled]=\"data.visualizationMode\" *ngFor=\"let area of listadoTemporal\" [value]=\"area.idCargo\">\r\n          {{area.descripcion}}</mat-option>\r\n\r\n      </mat-select>\r\n    </mat-form-field>\r\n\r\n  </form>\r\n\r\n\r\n\r\n</div>\r\n\r\n\r\n\r\n<div mat-dialog-actions fxLayoutAlign=\"end center\" fxLayoutGap=\"8px\">\r\n  <button *ngIf=\"!data.visualizationMode\" style=\"color: #fff; background-color: #EF2E2E;\" mat-raised-button\r\n    (click)=\"onNoClick()\">Cancelar</button>\r\n  <button color=\"primary\" mat-raised-button [disabled]=\"!formAddAreaSubArea.valid\"\r\n    (click)=\"saveAreaSubArea(formAddAreaSubArea.value)\" cdkFocusInitial> {{data.visualizationMode ? 'Cerrar' :\r\n    'Guardar'}}</button>\r\n</div>","import { NgModule } from \"@angular/core\";\r\nimport { R